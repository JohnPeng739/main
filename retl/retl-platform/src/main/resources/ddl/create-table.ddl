DROP TABLE IF EXISTS TB_SRC;

CREATE TABLE TB_SRC (
  ID CHAR(40) NOT NULL,
  CODE VARCHAR(30) NOT NULL,
  NAME VARCHAR(120),
  FAMILIES TINYINT,
  AGE SMALLINT,
  MONEY DECIMAL(10, 2),
  BIRTHDAY DATE,
  CREATEDTIME TIMESTAMP(3),
  UPDATEDTIME TIMESTAMP(3),
  EMAIL VARCHAR(50),
  MARRIED BOOL,
  INDEXNO BIGINT(8),
  PRIMARY KEY (ID),
  KEY INDEX_ID(ID),
  KEY INDEX_CODE (CODE),
  KEY INDEX_CREATEDTIME (CREATEDTIME),
  KEY INDEX_UPDATEDTIME (UPDATEDTIME),
  KEY INDEX_INDEXNO (INDEXNO)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

