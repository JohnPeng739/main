  CREATEDTIME TIMESTAMP(3),
  STRUCTUREDTIME BIGINT(8),
  VALIDATEDTIME BIGINT(8),
  TRANSFORMEDTIME BIGINT(8),
  PERSISTEDTIME BIGINT(8),
  ERRORTIME BIGINT(8),
  PRIMARY KEY (ID),
  KEY INDEX_ID (ID),
  KEY INDEX_FIELDNAME (FIELDNAME),
  KEY INDEX_CREATEDTIME (CREATEDTIME),
  KEY INDEX_ERRORTIME (ERRORTIME)
