DROP TABLE IF EXISTS TB_ERRORS;

CREATE TABLE TB_ERRORS (
  ID CHAR(40) NOT NULL,
  TYPE VARCHAR(20) NOT NULL,
  MESSAGE VARCHAR(500),
  DATA VARCHAR(4000),
